# Clerk Authentication Keys (BOTH required for Next.js App Router)
# Publishable Key - for client-side components
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Secret Key - for server-side components and middleware
CLERK_SECRET_KEY=sk_test_your_secret_key_here

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:5000/api
